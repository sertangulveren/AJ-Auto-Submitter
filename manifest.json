{
  "manifest_version": 2,
  "name": "AJ Flight Inquiry",
  "description": "An extention to automatically transfer parameters to the Anadolu Jet's search form.",
  "version": "0.1.1",
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "AJ Flight Inquiry"
  },
  "permissions": [
    "*://*/*",
    "contextMenus",
    "idle",
    "tabs"
  ]
}

